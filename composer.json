{
    "name": "romanzipp/laravel-projectable-aggregates",
    "description": "Projectable aggregates for Laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "hey@romanzipp.com",
            "homepage": "https://romanzipp.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\ProjectableAggregates\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\ProjectableAggregates\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\ProjectableAggregates\\Providers\\ProjectableAggregatesProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
